<div class="sign-in">
    <form action="/signin" method="POST">
        <h2>Sign In</h2>
        <hr>
        <div class="signin-inputs">
            <label for="emailField">Email</label><br>
            <input type="email" name="email" class="form-control" id="emailField" aria-describedby="emailHelp" placeholder="Enter email">
            <br>
            <label for="passwordField">Password</label><br>
            <input type="password" name="password" class="form-control" id="passwordField" placeholder="Password">
        </div>
            <button class="signin-button" type="submit">Login</button>
    </form>
    <div>
        <a href="/register">
        <button class="new-register-button" type="button">New? Register Here!</button></a>
    </div>
</div>
